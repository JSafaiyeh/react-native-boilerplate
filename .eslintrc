{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "globals": {
        "__DEV__": true
    },
    "plugins": [
        "react",
        "react-native",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "no-use-before-define": 0,
        "react/jsx-filename-extension": 0,
        "react/forbid-prop-types": [2, { "forbid": ["array"] }],
        "react-native/no-unused-styles": 2,
        "react-native/no-inline-styles": 2
    }
}